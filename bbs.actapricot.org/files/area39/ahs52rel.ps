	/fontdict exch def	% font dictionary
	% /charname
	fontdict begin
	Encoding char get 	% name
	fontdict /Metrics get exch get 0  	% xwidth 0
	% 4 { pop } repeat
	setcharwidth
	char			% integer
	Encoding char get	% integer name
	CharProcs exch get	% integer {}
	exec
	end
} bind def
end		% end of newfont use

